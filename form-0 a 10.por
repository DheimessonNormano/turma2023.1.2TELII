programa {
    // Imprimir de 1 a 10. 
    inteiro total
	funcao inicio() 
	{
	       para(inteiro numero = 0; numero < 10; numero++)
	       {
	           total = numero + 1
	           escreva ("Número: ",total, "\n")
	       }
	       // escrever o Total fora do Loop:
	       escreva("\nTotal: ", total)
	}
}	
	